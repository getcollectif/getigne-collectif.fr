# Supabase
VITE_SUPABASE_URL=your_supabase_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key
# Optionnel : pour make supabase_ensure_buckets (création des buckets en projet hébergé). En local, l’anon key suffit si .env pointe vers l’URL locale.
# SUPABASE_SERVICE_ROLE_KEY=...

# Discord
VITE_DISCORD_WEBHOOK_URL=your_discord_webhook_url
VITE_DISCORD_INVITE_URL=your_discord_invite_url

# Réseaux sociaux
VITE_FACEBOOK_URL=your_facebook_url
VITE_INSTAGRAM_URL=your_instagram_url

# PostHog
VITE_POSTHOG_KEY=your_posthog_key
VITE_POSTHOG_HOST=your_posthog_host

# Mailchimp (configuré dans Supabase Edge Functions)
# Ces variables doivent être configurées dans Supabase, pas ici :
# MAILCHIMP_API_KEY=your_mailchimp_api_key
# MAILCHIMP_SERVER_PREFIX=your_server_prefix  
# MAILCHIMP_AUDIENCE_ID=your_audience_id
